{
    "compilerOptions": {
      "target": "ESNext",
      "composite": true,
      "useDefineForClassFields": true,
      "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
      ],
      "declaration": true,
      "declarationMap": true,
      "allowJs": false,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react",
      "types": ["jest"],
      "outDir": "dist"
    },
    "include": []
  }
  